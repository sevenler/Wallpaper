<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <clip>
            <shape>
                <corners android:radius="15dip" />

                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#dbdbdb"
                    android:startColor="#dbdbdb" />

                <stroke
                    android:width="1dp"
                    android:color="#aeaeae" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="15dip" />

                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#005f09"
                    android:startColor="#baff00" />

                <stroke
                    android:width="1dp"
                    android:color="#aeaeae" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerY="0.25"
                    android:endColor="#fedc1d"
                    android:startColor="#fedc1d" />

                <corners android:radius="15dip" />

                <stroke
                    android:width="1dp"
                    android:color="#fedc1d" />
            </shape>
        </clip>
    </item>

</layer-list>